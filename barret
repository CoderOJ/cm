#ifndef CM_BARRET_H
#define CM_BARRET_H
#include <cstdint>
namespace cm{struct barret_reduction{private:uint64_t _4;uint64_t _5;int _6;static uint64_t _2(uint64_t a,uint64_t b){return(_3(a)*b)>>64;}public:barret_reduction(uint64_t mod){this->_4=mod;this->_6=63-_1(mod);this->_5=static_cast<uint64_t>((_3(1)<<(_6+64))/mod)+1;}uint64_t div(uint64_t x){return _2(x,_5)>>_6;}uint64_t mod(uint64_t x){return x-div(x)*_4;}};}
#endif
